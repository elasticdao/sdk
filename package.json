{
  "name": "@elastic-dao/sdk",
  "version": "0.0.1",
  "description": "SDK for ElasticDAO contracts and core modules",
  "main": "index.js",
  "repository": "https://github.com/elasticdao/sdk",
  "author": "Dan Matthews <dan@cryptobuilders.io>",
  "license": "MIT",
  "private": false,

  "scripts": {
    "artifacts": "bash ./scripts/copyArtifacts.sh",
    "lint": "yarn prettier && npx eslint src/*.js src/**/*.js src/**/**/*.js src/**/**/**/*.js",
    "prerelease": "yarn workflow:lint",
    "build": "rollup -c",
    "dev": "yarn artifacts && yarn lint && rollup -c -w",
    "prettier": "yarn artifacts && npx prettier --write src/*.js --write src/**/*.js --write src/**/**/*.js --write src/**/**/**/*.js",
    "start": "sirv public",
    "workflow:lint": "bash ./scripts/setupArtifacts.sh && yarn lint"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.1",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.0"
  },
  "dependencies": {
    "@pie-dao/utils": "^1.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "bignumber.js": "^9.0.0",
    "ethers": "^5.0.9",
    "sirv-cli": "^1.0.0"
  }
}
